{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "standard"
  ],
  "rules": {
    "semi": [2, "always"],
    "semi-spacing": [2, { "before": false, "after": true }],
    "space-before-function-paren": [2, {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
    }],
    "no-multiple-empty-lines": [2, { max: 3, maxBOF: 0, maxEOF: 1 }],
    "curly": [2, "all"]
  }
}
